<UserControl x:Class="GraphAlgorithmRenderer.UIControls.Properties.ArrowUiProperty"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GraphAlgorithmRenderer.UIControls.Properties"
             mc:Ignorable="d">
    <StackPanel x:Name="Panel" Orientation="Vertical">
        <CheckBox x:Name="CheckBox">Orientation</CheckBox>
        <Separator></Separator>
        <CheckBox x:Name="Target">Arrow at target</CheckBox>
        <CheckBox x:Name="Source">Arrow at source</CheckBox>
    </StackPanel>
</UserControl>
