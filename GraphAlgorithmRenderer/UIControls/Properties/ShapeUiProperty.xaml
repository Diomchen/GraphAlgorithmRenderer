<UserControl x:Class="GraphAlgorithmRenderer.UIControls.Properties.ShapeUiProperty"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GraphAlgorithmRenderer.UIControls.Properties"
             mc:Ignorable="d">

    <StackPanel x:Name="Panel" Orientation="Vertical">
        <CheckBox x:Name="CheckBox">Shape</CheckBox>
        <Separator></Separator>
        <RadioButton>Box</RadioButton>
        <RadioButton>Circle</RadioButton>
        <RadioButton>Diamond</RadioButton>
        <RadioButton>Double Circle</RadioButton>
        <RadioButton>Ellipse</RadioButton>
        <RadioButton>House</RadioButton>
        <RadioButton>Octagon</RadioButton>
        <RadioButton>Parallelogram</RadioButton>
        <RadioButton>Point</RadioButton>
        <RadioButton>Polygon</RadioButton>
        <RadioButton>Trapezium</RadioButton>
        <RadioButton>Triangle</RadioButton>
    </StackPanel>
</UserControl>
